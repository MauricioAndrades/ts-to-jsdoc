{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["google"],
    "parserOptions": { "ecmaVersion": 12 },
    "rules": {
        "max-len": "warn",
        "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "quotes": ["error", "double"],
        "indent": ["error", "tab"]
    }
}
